            <div class="container" data-section="cms-services">
                <div class="section-title text-center">
                    {% if section.settings.heading != blank %}<h2>{{ section.settings.heading }}</h2>{% endif %}
                    {% if section.settings.content != blank %}<p>{{ section.settings.content }}</p>{% endif %}
                </div>
                <div class="row">
                    {% if section.blocks.size > 0 %}
                    {% for block in section.blocks %}
                    <div class="col-12 col-sm-6 col-md-4">
                        <div class="about-us-box"> 
                            {% if block.settings.image != blank %}<img src="{{ block.settings.image | img_url : 'master' }}">{% endif %}
                           {% if block.settings.title != blank %} <h3>{{ block.settings.title }}</h3>{% endif %}
                           {% if block.settings.description != blank %} <p>{{ block.settings.description }}</p>{% endif %}
                        </div>
                    </div>
                    {% endfor %}
                    {% endif %}
                    {% if section.blocks.size == 0 %}
                        {% include 'no-blocks' %}
                    {% endif %}
                </div>
            </div>

{% schema %}
  {
  "name": "cms-services",
  "tag": "section",
  "class": "aboutpage-btm-sec about-us-section padding-top",
  "max_blocks": 3, 
  "settings":[
    {
      "type": "text",
      "id": "heading",
      "label": "t:settings_schema.Our-partners.settings.heading"
    },
    {
      "type": "text",
      "id": "content",
      "label": "t:settings_schema.Our-partners.settings.content"
    }
  ],
  "blocks": [
    {
      "type": "Services",
      "name": "cms-services",
      "settings":[
          {
            "type": "image_picker",
            "id": "image",
            "label": "t:settings_schema.Our-partners.blocks.image"
          },
          {
            "type": "text",
            "id": "title",
            "label": "t:settings_schema.Our-partners.blocks.title"
          },
          {
            "type": "text",
            "id": "description",
            "label": "t:settings_schema.Our-partners.blocks.description"
          }
       ]
    }
  ],
  "presets": [
    {
      "name": "cms services"
    }
  ]
}
{% endschema %}